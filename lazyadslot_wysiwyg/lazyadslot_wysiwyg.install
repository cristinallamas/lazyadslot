<?php
/**
 * Implements hook_install().
 */
function lazyadslot_wysiwyg_install() {
  variable_set('lazyadslot_wysiwyg_placeholder', '<div class="inline-body-placeholder"></div>');
}

/**
 * Implements hook_uninstall().
 */
function lazyadslot_wysiwyg_uninstall() {
  variable_del('lazyadslot_wysiwyg_placeholder');
}
